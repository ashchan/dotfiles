--type-set=coffee=.coffee
--type-set=haml=.haml
--type-set=scss=.scss
--type-set=sass=.sass
--type-set=less=.less
--type-set=jade=.jade
--type-set=log=.log
--type-set=minifiedjs=.min.js

--nominifiedjs

--ignore-dir=log/
--ignore-dir=lib-cov/
--ignore-dir=node_modules/
